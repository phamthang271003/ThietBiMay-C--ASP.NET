@model IEnumerable<Đồ_án_lập_trình_web.Models.Category>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <th>
            Category ID
        </th>
        <th>
            Category Name
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>

        </tr>
    }

</table>
